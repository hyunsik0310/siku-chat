{
  "expo": {
    "name": "siku-chat",
    "slug": "siku-chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/siku_moji.png",
      "resizeMode": "contain",
      "backgroundColor": "#3A53D8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.siku.sikuchat",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "The app accesses to photo library to upload your profile photo when you are signing up or updating the profile."
      }
    },
    "android": {
      "package": "com.siku.sikuchat",
      "versionCode": 1,
      "permissions": ["CAMERA"],
      "icon": "./assets/splash.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
